---
import { social } from '~/constants/social';
---

<section class="bg-offset h-[20rem]">
  <iframe
    src={social.embedUrl}
    width="100%"
    title="Localisation"
    height="100%"
    style="border:0;"
    allowfullscreen={false}
    loading="lazy"
    referrerpolicy="no-referrer-when-downgrade"></iframe>
</section>
